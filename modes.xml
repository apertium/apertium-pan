<?xml version="1.0" encoding="UTF-8"?>
<modes>

  <mode name="pan_Arab-morph" install="yes">
    <pipeline>
      <program name="lt-proc -w">
        <file name="pan_Arab.automorf.bin"/>
      </program>
    </pipeline>
  </mode>

  <mode name="pan_Arab-gener" install="yes">
    <pipeline>
      <program name="lt-proc -g">
        <file name="pan_Arab.autogen.bin"/>
      </program>
    </pipeline>
  </mode>

  <mode name="pan_Arab-tagger" install="yes">
    <pipeline>
      <program name="lt-proc -w">
        <file name="pan_Arab.automorf.bin"/>
      </program>
      <program name="cg-proc -w">
        <file name="pan.rlx.bin"/>
      </program>
    </pipeline>
  </mode>

  <mode name="pan_Arab-disam" install="yes">
    <pipeline>
      <program name="lt-proc -w">
        <file name="pan_Arab.automorf.bin"/>
      </program>
      <program name="cg-conv -a -l" />
      <program name="vislcg3 --trace -g">
        <file name="apertium-pan.pan.rlx"/>
      </program>
    </pipeline>
  </mode>

  <mode name="pan_Guru-morph" install="yes">
    <pipeline>
      <program name="lt-proc -w">
        <file name="pan_Guru.automorf.bin"/>
      </program>
    </pipeline>
  </mode>

  <mode name="pan_Guru-gener" install="yes">
    <pipeline>
      <program name="lt-proc -g">
        <file name="pan_Guru.autogen.bin"/>
      </program>
    </pipeline>
  </mode>

  <mode name="pan_Guru-tagger" install="yes">
    <pipeline>
      <program name="lt-proc -w">
        <file name="pan_Guru.automorf.bin"/>
      </program>
      <program name="cg-proc -w">
        <file name="pan.rlx.bin"/>
      </program>
    </pipeline>
  </mode>

  <mode name="pan_Guru-disam" install="yes">
    <pipeline>
      <program name="lt-proc -w">
        <file name="pan_Guru.automorf.bin"/>
      </program>
      <program name="cg-conv -a -l" />
      <program name="vislcg3 --trace -g">
        <file name="apertium-pan.pan.rlx"/>
      </program>
    </pipeline>
  </mode>



</modes>

